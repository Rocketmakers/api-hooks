{
  "name": "api-hooks-playground",
  "version": "1.0.0",
  "description": "Front-end React playground area for testing the API Hooks library",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "concurrently \"npm:type-check\" \"vite --host\" -n \"TYPE CHECK,VITE\"",
    "postinstall": "npm run link-api-hooks",
    "link-api-hooks": "npm link @rocketmakers/api-hooks",
    "type-check": "tsc -p ./tsconfig.json --noEmit --watch --preserveWatchOutput"
  },
  "author": "Rocketmakers",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@rocketmakers/armstrong-edge": "^1.14.0",
    "history": "^5.3.0",
    "react-router-dom": "^6.3.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "concurrently": "7.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8"
  }
}
